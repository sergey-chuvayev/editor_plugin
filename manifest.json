{
  "manifest_version": 2,
  "name": "Editor Plugin",
  "version": "0.1",
  "browser_action": {
   "default_icon": "img/icon.png"
  },
  "permissions": [
    "activeTab"
    ],
  "content_scripts": [
    {
      "matches": [ "http://www.the-village.ru/posts/*/edit" ],
      "js": ["lib/jquery.js", "lib/jquery.mousewheel.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}